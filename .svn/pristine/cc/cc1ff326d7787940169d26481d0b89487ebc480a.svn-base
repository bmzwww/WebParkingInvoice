

<div class="well">
    <h2> @ViewBag.Title</h2>
    <p class="lead">@ViewBag.Message</p>
    <div ng-controller="IndexController">
        <table class="table table-hover table-bordered">
            <thead>
                <tr>
                    <th>
                        #
                    </th>
                    <th>
                        Car number
                    </th>
                    <th>
                        Place
                    </th>
                    <th>
                        Period start
                    </th>
                    <th>
                        Period end
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr role="row" ng-repeat="p in ParkInfo.Items">
                    <td>
                        {{ p.ID }}
                    </td>
                    <td>
                        {{ p.CarNumber }}
                    </td>
                    <td>
                        {{ p.ParkHouse.Name }}
                    </td>
                    <td>
                        {{ p.PeriodStart }}
                    </td>
                    <td>
                        {{  p.PeriodEnd }}
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
